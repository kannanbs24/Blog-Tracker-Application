@model IEnumerable<UI_layer.Models.blogmodel>

<center>
    <h2><b>List Of Blogs</b></h2></center>

    @foreach (var item in Model)
    {
      
           <div class="container">

               <h1>   @Html.DisplayFor(modelItem => item.Title)</h1>

               <h1>@Html.DisplayFor(modelItem => item.Subject)</h1>

               <h1> @Html.DisplayFor(modelItem => item.DateOfCreation)</h1>

               <h1> @Html.DisplayFor(modelItem => item.BlogUrl)</h1>

               <h1>  @Html.DisplayFor(modelItem => item.EmpEmailId)</h1>
           </div>
      
        <br/>
        <br />
    }

